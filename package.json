{
  "name": "@productiongrade/passkeys",
  "version": "1.1.0",
  "description": "Production-ready passkey authentication library for Node.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/express/index.d.ts",
      "require": "./dist/express/index.js",
      "import": "./dist/express/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "require": "./dist/react/index.js",
      "import": "./dist/react/index.js"
    },
    "./adapters/prisma": {
      "types": "./dist/adapters/prisma/index.d.ts",
      "require": "./dist/adapters/prisma/index.js",
      "import": "./dist/adapters/prisma/index.js"
    },
    "./adapters/redis": {
      "types": "./dist/adapters/redis/index.d.ts",
      "require": "./dist/adapters/redis/index.js",
      "import": "./dist/adapters/redis/index.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs/index.d.ts",
      "require": "./dist/nextjs/index.js",
      "import": "./dist/nextjs/index.js"
    },
    "./nextjs/provider": {
      "types": "./dist/nextjs/provider.d.ts",
      "require": "./dist/nextjs/provider.js",
      "import": "./dist/nextjs/provider.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "require": "./dist/testing/index.js",
      "import": "./dist/testing/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "passkeys",
    "webauthn",
    "fido2",
    "authentication",
    "passwordless",
    "biometrics",
    "security",
    "express",
    "nextjs",
    "next-auth",
    "react",
    "typescript"
  ],
  "author": "ProductionGrade",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Production-Grade/passkeys"
  },
  "bugs": {
    "url": "https://github.com/Production-Grade/passkeys/issues"
  },
  "homepage": "https://github.com/Production-Grade/passkeys#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "prisma/schema.prisma"
  ],
  "dependencies": {
    "@simplewebauthn/server": "^9.0.0",
    "bcrypt": "^5.1.1"
  },
  "peerDependencies": {
    "@simplewebauthn/browser": "^9.0.0",
    "express": "^4.18.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.0",
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "next-auth": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@simplewebauthn/browser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@prisma/client": "^5.0.0",
    "@simplewebauthn/browser": "^9.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.18.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.4.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redis": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
